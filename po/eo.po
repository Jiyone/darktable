msgstr "14 EV (kontraŭlumigo)"

#: ../src/iop/filmic.c:354
msgid "15 EV (sunset)"
msgstr "15 EV (sunsubiro)"

#: ../src/iop/filmic.c:362
msgid "16 EV (HDR)"
msgstr "16 EV (ADG)"

#: ../src/iop/filmic.c:370
